{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "apps",
  "projects": {
    "market": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss",
          "skipTests": true
        },
        "@schematics/angular:class": {
          "skipTests": true
        },
        "@schematics/angular:directive": {
          "skipTests": true
        },
        "@schematics/angular:guard": {
          "skipTests": true
        },
        "@schematics/angular:interceptor": {
          "skipTests": true
        },
        "@schematics/angular:pipe": {
          "skipTests": true
        },
        "@schematics/angular:resolver": {
          "skipTests": true
        },
        "@schematics/angular:service": {
          "skipTests": true
        }
      },
      "root": "apps/market",
      "sourceRoot": "apps/market/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/market",
            "index": "apps/market/src/index.html",
            "browser": "apps/market/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "apps/market/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "allowedCommonJsDependencies": [
              "crypto-js",
              "crypto-js/lib-typedarrays",
              "crypto-js/hmac-sha256",
              "sweetalert2",
              "apexcharts"
            ],
            "assets": [
              "apps/market/src/favicon.ico",
              "apps/market/src/assets"
            ],
            "styles": [
              "apps/market/src/styles/global.scss"
            ],
            "scripts": [
              "./node_modules/apexcharts/dist/apexcharts.min.js",
              "./node_modules/bootstrap/dist/js/bootstrap.bundle.min.js"
            ],
            "stylePreprocessorOptions": {
              "sass": {
                "silenceDeprecations": ["mixed-decls", "color-functions", "global-builtin", "import"]
              }
            }
          },
          "configurations": {
            "production": {
              "optimization": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "4mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kb",
                  "maximumError": "5kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/market/src/environments/environment.ts",
                  "with": "apps/market/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "hosting": {
              "optimization": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "4mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kb",
                  "maximumError": "5kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/market/src/environments/environment.ts",
                  "with": "apps/market/src/environments/environment.host.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "market:build:production"
            },
            "development": {
              "buildTarget": "market:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "market:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "apps/market/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/market/src/favicon.ico",
              "apps/market/src/assets"
            ],
            "styles": [
              "apps/market/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
    "market-commons": {
      "projectType": "library",
      "root": "libs/market-commons",
      "sourceRoot": "libs/market-commons/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "libs/market-commons/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/market-commons/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/market-commons/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "libs/market-commons/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "6f116713-2a45-4866-a5f7-2606e3f788d1",
    "cache": {
      "enabled": false
    }
  }
}